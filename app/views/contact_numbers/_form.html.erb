<div class="row">
  <div class="col-lg-4 left-move"></div>
  <div class="col-lg-4 left-move">
    <%= f.number_field :number, class: 'numberfield' %>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="row">
  <div class="col-lg-4 left-move"></div>
  <div class="col-lg-4 left-move">
    <label for="business_contact_number">Contact Type</label>
    <%= f.select :contact_type,options_for_select([ "Main", "Cell" , "Direct", "Fax" , "Home" , "Main" , "Phone Number" , "Phone Number 2" , "Toll Free" ], @number[:contact_type]) ,{ :include_blank => '-- Select One --',hide_label: true }%>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="actions col-lg-12">
  <div class="row">
    <div class="col-lg-5 left-move"></div>
    <div class="left-move">
      <%= f.submit value: "Submit", class: "btn btn-primary"%>
    </div>
    <div class="ml-2">
      <%= link_to 'Cancel', generals_path, class: "btn btn-secondary " %>
    </div>
  </div>
</div>
