	<div class="row">
		<div class="col-lg-4 left-move"></div>
		<div class="col-lg-4 left-move">
			<%=f.select :plan, options_for_select([ "Administrative forms", "Compilance Reports","Legal Plan Documents","Feduciary Documents"], @feduciary_document[:plan]), { :include_blank => '-- Select One --' }%>
			<div class="invalid-feedback">Please provide a time.</div>
			<div class="valid-feedback">Looks good!</div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-4 left-move"></div>
		<div class="col-lg-4 left-move">
			<%=f.select :feduciary_document_type, options_for_select([ "Annual Valuation Report", "Benificiary Form","Summer Valuation Report","Fund Performance Report"]), { :include_blank => '-- Select One --' }%>
			<div class="invalid-feedback">Please provide a time.</div>
			<div class="valid-feedback">Looks good!</div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-4 left-move"></div>
		<div class="col-lg-4 left-move">
			<%=f.text_area :description%>
			<div class="invalid-feedback">Please provide a time.</div>
			<div class="valid-feedback">Looks good!</div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-4 left-move"></div>
		<div class="col-lg-4 left-move">
			<%=f.file_field :feduciary_doc%>
			<div class="invalid-feedback">Please provide a time.</div>
			<div class="valid-feedback">Looks good!</div>
		</div>
	</div>

	<div class="row">
		<div class="col-lg-4 left-move"></div>
		<div class="col-lg-4 left-move">
			<%= f.date_field :effective_date %>
			<div class="invalid-feedback">Please provide a time.</div>
			<div class="valid-feedback">Looks good!</div>
		</div>
	</div>

	<div class="row">
		<div class="col-lg-4 left-move"></div>
		<div class="col-lg-4 left-move">
			<%= f.date_field :period_end%>
			<div class="invalid-feedback">Please provide a time.</div>
			<div class="valid-feedback">Looks good!</div>
		</div>
	</div>
	<div class="actions col-lg-12">
		<div class="row">
			<div class="col-lg-5 left-move"></div>
			<div class="left-move">
				<%= f.submit value: "Save", class: "btn btn-primary", id:"save_document"%>
			</div>
			<div class="col-lg-1 right-move"></div>
			<div class="right-move">
				<%= link_to 'Cancel', root_path, class: "btn btn-secondary " %>
			</div>
		</div>
	</div>

