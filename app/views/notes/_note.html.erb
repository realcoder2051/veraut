<tr class="redirect_to_show cursor-pointer" data-url="<%= note_path(note) %>">
	<td><%= note.description%></td>
	<td><%= note.created_at %></td>
	<td><%= note.created_by %></td>
	<td>
		<a data-confirm="Are you sure?" rel="nofollow" class="delete_href" data-id=<%=note.id%>><i
				class="material-icons" class="delete_icon" >delete</i></a>
	</td>
</tr>