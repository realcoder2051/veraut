<div class="form-row" >
  <div class="col-lg-3 left-move"></div>
  <div class="col-lg-4 left-move">
    <%=f.text_field :first_name , {:label=> 'First Name'}%>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="row" >
  <div class="col-lg-3 left-move"></div>
  <div class="col-lg-4 left-move">
    <%=f.text_field :last_name , {:label=> 'Last Name'}%>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="row" >
  <div class="col-lg-3 left-move"></div>
  <div class="col-lg-4 left-move ">
  <%=f.text_field :ssn,	{:maxlength => 9, :label=> 'SSN'}  %>
  <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="row" >
  <div class="col-lg-3 left-move"></div>
  <div class="col-lg-4 left-move">
    <%=f.select :gender,options_for_select(["Male","Female"], @employee[:gender]), { :include_blank => '-- Select One --' }%>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="row d-none" >
  <div class="col-lg-3 left-move"></div>
  <div class="col-lg-4 left-move" >
		<input type="text" class='datepicker' >
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>

<div class="row" >
  <div class="col-lg-3 left-move" ></div>
  <div class="col-lg-4 left-move">
    <%=f.date_field :date_of_birth , {:class=> 'date_picker'} %>
