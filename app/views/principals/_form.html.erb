 <div class="row" >
  <div class="col-lg-4 left-move"></div>
  <div class="col-lg-4 left-move">
    <%= f.text_field :name %>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="row" >
  <div class="col-lg-4 left-move"></div>
  <div class="col-lg-4 left-move">
    <%=f.select :title,options_for_select([ "Managing Partner", "Note Title", "Other Title", "Partner", "President" , "Secretary" , "Treasurer" , "Vice-President"], @principal[:title]) , { :include_blank => '-- Select One --' }  %>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>

<div class="row mt-3" >
  <div class="col-lg-4 left-move"></div>
  <div class="col-lg-4 left-move">
    <%= f.check_box :officer  %>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>

<div class="row mt-3 d-none" >
  <div class="col-lg-4 left-move"></div>
  <div class="col-lg-4 left-move">
		<div class="form-check d-flex align-items-center">
			<input id="principal_officer" class="form-check-input mt-0"  value="1" type="checkbox" name="principal[officer]" >
			<label  class="form-check-label ml-1 my-1" for="principal_officer" >Officer</label>
		</div>

    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="row mt-4" >
  <div class="col-lg-4 left-move"></div>
  <div class="col-lg-4 left-move">
    <%= f.number_field :ownership %>
    <div class="invalid-feedback">Please provide a time.</div>
    <div class="valid-feedback">Looks good!</div>
  </div>
</div>
<div class="actions col-lg-12">
  <div class="row">
    <div class="col-lg-5 left-move"></div>
    <div class="left-move">
      <%= f.submit value: "Save", class: "btn btn-primary"%>
    </div>
    <div class="col-lg-1 right-move"></div>
    <div class="right-move">
      <%= link_to 'Cancel', principals_path, class: "btn btn-secondary " %>
    </div>
  </div>
</div>
